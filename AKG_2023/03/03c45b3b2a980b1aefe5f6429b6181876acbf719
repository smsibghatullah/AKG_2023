/* /pos_create_product/static/src/js/ProductCreatePopupWidget.js defined in bundle 'point_of_sale.assets' */
odoo.define('pos_create_product.ProductCreatePopupWidget',function(require){"use strict";const AbstractAwaitablePopup=require('point_of_sale.AbstractAwaitablePopup');const{useState,useRef}=owl.hooks;const Registries=require('point_of_sale.Registries');const rpc=require('web.rpc');var pos_model=require('point_of_sale.models');var model_list=pos_model.PosModel.prototype.models

get headerButtonIsShown(){return!this.env.pos.config.module_pos_hr||this.env.pos.get('cashier').role=='manager'||this.env.pos.get_cashier().user_id[0]===this.env.pos.user.id;}
showCashMoveButton(){return super.showCashMoveButton()&&this.env.pos.get('cashier').role=='manager';}
